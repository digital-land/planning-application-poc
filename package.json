{
  "name": "development-plan-timetable",
  "version": "0.0.1",
  "description": "A prototype tool for to help users collect development plan timetable data",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "flask": "flask run",
    "watch": "npm-run-all --parallel watch:*",
    "nps": "nps -c ./node_modules/digital-land-frontend/package-scripts.js",
    "copyjs": "rsync -ar src/javascripts/ application/static/javascripts",
    "postinstall": "npm run copyjs && npm run nps copy.javascripts && npm run nps build.stylesheets && npm run nps build.javascripts",
    "start": "run-p watch flask"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digital-land/development-plan-timetable-prototype.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/digital-land/development-plan-timetable-prototype./issues"
  },
  "dependencies": {
    "digital-land-frontend": "https://gitpkg.now.sh/digital-land/digital-land-frontend/package?main",
    "govuk-frontend": "^4.0.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "standardx": "^7.0.0"
  }
}
